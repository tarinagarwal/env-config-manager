{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API for Environment Configuration Manager",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:reset": "prisma db push --force-reset && npm run prisma:seed",
    "db:setup": "npm run prisma:generate && npm run prisma:push && npm run prisma:seed",
    "db:verify": "tsx scripts/verify-db.ts",
    "generate-license": "tsx scripts/generate-license.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.901.0",
    "@aws-sdk/client-ssm": "^3.901.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@opentelemetry/exporter-prometheus": "^0.206.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.206.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@prisma/client": "^5.22.0",
    "@prisma/debug": "^6.17.0",
    "@sentry/node": "^10.18.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "ldapjs": "^3.0.7",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-saml": "^3.2.4",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "redis": "^5.8.3",
    "winston": "^3.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/ldapjs": "^3.0.6",
    "@types/node": "^20.10.6",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-saml": "^1.1.6",
    "@types/qrcode": "^1.5.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "ansi-styles": "^6.2.3",
    "chalk": "^5.6.2",
    "jest": "^29.7.0",
    "prisma": "^5.22.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
